
datasource db {
  provider = "mysql"
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

enum RoomPrivacy {
  public
  private
}

model Room {
  id         String     @id @default(cuid())
  privacy    RoomPrivacy @default(public)
  createdAt  DateTime    @default(now()) @map("created_at")
  updatedAt  DateTime    @updatedAt @map("updated_at")
  @@map("rooms")
}